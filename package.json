{
  "name": "react-next-boilerplate-site",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:react-next-boilerplate/react-next-boilerplate-site.git",
  "author": "Jorge Luis Calleja <jorgelca1205@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:js",
    "lint:js": "eslint src --fix",
    "prettier": "prettier --config .prettierrc \"./**/*.+(js|json|jsx)\" --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "./**/*.+(js|json|jsx)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^3.0.0-alpha.0",
    "@typefaces-pack/typeface-inter": "^1.0.3",
    "next": "^9.3.6",
    "next-pwa": "^2.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "swr": "^0.2.0",
    "typeface-metropolis": "^0.0.74",
    "typeface-roboto-mono": "^0.0.75"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.4",
    "prettier": "^1.19.1"
  }
}
